
public class Data_type {
	
	
	
	
	public static void main(String args[])
	
	
	{
	
		
		int num=-200;
		
		num=90;
		System.out.println(num);
		
		num=30;
		System.out.println(num);
		num=0;
		System.out.println(num);
		num=87;
		System.out.println(num);
		
		double a=3.5;
		char j='P';
		String name="john";
		
		long b=234324343;
		
		System.out.println(num);
		
		
		
	
	}
	
	
		
}